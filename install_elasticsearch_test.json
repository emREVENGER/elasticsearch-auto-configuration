{
  "elasticsearch" : {
      "lang": "fr",
      "install": {
        "download_url": "https://artifacts.elastic.co/downloads/elasticsearch/",
        "version": "5.4.1",
        "as_service": true,
        "service_name": "elastic-5",
        "custom_directory": ""
      },
      "configuration": {
          "host": "http://localhost",
          "port": "9200",
          "commands" : [
              {
                "type": "_template",
                "name": "test_template",
                "expand_url_with_name": true,
                "file": "test/template_test.json",
                "method": "POST"
              },
              {
                "type": "_alias",
                "name": "test_alias",
                "expand_url_with_name": true,
                "file": "test/test_alias.json",
                "method": "POST"
              },
              {
                "type": "_aliases",
                "name": "test_aliases",
                "expand_url_with_name": false,
                "file": "test/test_aliases.json",
                "method": "POST"
              }
            ]
      }
   }
}
